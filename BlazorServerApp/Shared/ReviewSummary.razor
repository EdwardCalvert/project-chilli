@if (Models.Review.ReturnStars(DisplayReviewModel) != "")
{
@TitleText @Models.Review.ReturnStars(DisplayReviewModel)
}
            else
            {
                @("No reviews yet.")
            }
@code {
    [Parameter]
    public List<Review> DisplayReviewModel { get; set; }

    [Parameter]
    public string TitleText { get; set; }
}
